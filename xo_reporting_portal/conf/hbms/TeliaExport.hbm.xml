<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class  name="com.xo.web.ext.teliaexport.models.TeliaExport"  table="report_filters">
    <id name="exportId" type="java.lang.Integer">
            <column name="export_id" />
            <generator class="identity" />
    </id>
    <property name="createdDate" type="timestamp">
      <column length="100" name="created_date"/>
    </property>
    <many-to-one class="com.xo.web.models.system.User" fetch="select" name="user">
      <column length="40" name="user_id" not-null="true"/>
    </many-to-one>
    <property name="noOfUsers" type="java.lang.Integer">
      <column length="11" name="no_of_users" />
    </property>
    <property name="dateOfEvent" type="string">
      <column length="30" name="date_of_event" />
    </property>
  </class>
</hibernate-mapping>